        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.814"
        app:srcCompat="@drawable/papelzinho1__1__removebg_preview" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#00000000"
        app:srcCompat="@drawable/casa__4_"
        tools:layout_editor_absoluteX="338dp"
        tools:layout_editor_absoluteY="654dp" />

    <TextView
        android:id="@+id/fofoca1"
        android:layout_width="229dp"
        android:layout_height="194dp"
        android:text="@string/fofoca1"
